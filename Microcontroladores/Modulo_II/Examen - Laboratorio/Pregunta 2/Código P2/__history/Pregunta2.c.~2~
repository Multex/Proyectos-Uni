#include <16f877a.h>
#fuses xt
#use delay(clock = 4M)
#byte trisb = 0x86
#byte trisb = 0x87
#byte portb = 0x06
#byte portb = 0x07

void main() {
   
   int datos[10] = {0b11000000, 0b11111001, 0b10101100, 0b10110000, 0b10011001,
                    0b10010010, 0b10000010, 0b11111000, 0b10000000, 0b10010000};
   int i;
   
   trisb = 0;
   trisb = 0;
   portb = 0;
   portb = 0;
   
   while(true){
      for(i = 0; i < 10; i++) {
         portb = datos[i];
      }
   }
}
